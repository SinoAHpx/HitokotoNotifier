<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        x:Class="HitokotoNotifier.MainWindow" Width="180" Height="80" WindowStartupLocation="CenterScreen" Icon="/favicon.ico"
        Title="HitokotoNotifier">
    <StackPanel Margin="10">
        <CheckBox Checked="AutoStartCheckBox_OnChecked" Unchecked="AutoStartCheckBox_OnUnchecked" Name="AutoStartCheckBox" Content="Auto Start"/>
        <!-- <TextBox TextInput="IntervalTextBox_OnTextInput" Name="IntervalTextBox" Watermark="Interval(seconds)" Text="3600" Margin="0,0,0,6" UseFloatingWatermark="True"/> -->
        <Button HorizontalAlignment="Stretch" Content="About" Click="Button_OnClick"/>
    </StackPanel>
</Window>
